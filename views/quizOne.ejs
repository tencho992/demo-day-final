
<% for (i=0; i<testQuestions.length; i++){ %>
  <p> <%= testQuestions[i].question %></p>   
  <img src="<%= testQuestions[i].img%>">
  <img id="one"  src="<%= testQuestions[i].character[0]%>" value="1">
  <img id="two" src="<%= testQuestions[i].character[1]%>" value="2">
  <img id="three" src="<%= testQuestions[i].character[2]%>" value="3">
  <button id="next" hidden class="e-btn">Next</button>
  <span hidden id="index"><%= testQuestions[i].correctIndex%></span>
<%}%>

